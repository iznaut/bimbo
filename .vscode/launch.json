{
    "configurations": [
        {
            "name": "Launch Electron App",
            "program": "${workspaceFolder}/main.js",
            "request": "launch",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "runtimeExecutable": "${workspaceFolder}/node_modules/.bin/electronmon",
            "type": "node",
            "args": ["--trace-warnings", "--dev"]
        }
    ]
}